      "@lexical/markdown": "0.16",
      "@lexical/overflow": "0.16",
      "@lexical/plain-text": "0.16",
      "@lexical/rich-text": "0.16",
      "@lexical/selection": "0.16",
      "@lexical/table": "0.16",
      "@lexical/text": "0.16",
      "@lexical/utils": "0.16",
      "@lexical/yjs": "0.16"
